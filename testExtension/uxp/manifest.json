{
    "name": "Photoshop UXP Starter Extension",
    "id": "com.adobe.photoshop.uxp_starter",
    "version": "0.1.0",
    "main": "index.html",
    "host": [
        {
            "app": "PS",
            "minVersion": "21.0.0"
        }
    ],
    "manifestVersion": 4,
    "entrypoints": [
        {
            "type": "panel",
            "id": "react-typescript",
            "label": {
                "default": "Starter"
            },
            "minimumSize": {
                "width": 230,
                "height": 200
            },
            "maximumSize": {
                "width": 2000,
                "height": 2000
            },
            "preferredDockedSize": {
                "width": 230,
                "height": 300
            },
            "preferredFloatingSize": {
                "width": 230,
                "height": 300
            },
            "commands": [
                {
                    "label": "Command 1",
                    "id": "flyoutMenuCommand1"
                }
            ]
        }
    ],
    "icons": [
        { "width": 32, "height": 32, "path": "icons/icon_D.png", "scale": [ 1, 2 ], "theme": [ "dark", "darkest" ], "species": [ "generic" ] },
        { "width": 32, "height": 32, "path": "icons/icon_N.png", "scale": [ 1, 2 ], "theme": [ "lightest", "light" ], "species": [ "generic" ] }
    ],
    "runOnStartup": true
}